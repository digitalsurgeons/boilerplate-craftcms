{# the ppBrowserType determines what type of image browser to show: 0 = just user media, 1 = just serach, 2 = both #}
{%- set ppBrowserType = (settings.ppBrowserType is defined ? settings.ppBrowserType : '0') -%}
<style>
/* Set style for each thumbnail coming into the modal */

#pp-thumbs .toolbar {
	margin: 2px 7px;
	background-repeat: no-repeat;
	background-size: 15px 15px;
	background-image: url({{ resourceUrl('picpuller/pp-icon.png') }});
}

.pp-search {
	position: absolute;
	top: 0;
	right: 0;
	font-family: HelveticaNeue, sans-serif !important;
	font-size: 11px;
}

.pp-search .btn.submit{
	overflow: visible;
	margin-left: 2px;
}

#pp-thumbs .toolbar h3 {
	margin-left: 21px;
}

.igpic {
	float: left;
	height: 100px;
	width: 100px;
	margin: 7px;
	border-radius: 3px;
	-webkit-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	-moz-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	overflow: hidden;
	border: #ccc 1px solid;
	position: relative;
}

.igpic.selectable {
	cursor: copy;
}

.igpic div.video, .igpp.preview .video
{
    border-radius: 3px;
    overflow: hidden;
    opacity: 0.75;
    background-image: url(data:image/png;base64,);
width:20px;
height:16px;
position: absolute;
top: 2px;
left: 2px;
}

.igpic.igpic_next.btn, .igpic.igpic_next.btn:active {
	border: 0;
	background-position: center right;
    background-image: url(data:image/png;base64,);
}

.igpic.igpic_next.btn:hover {
	background-position: center left;
}

.igpic.igpic_end {
	border: 0;
	background-image: url(data:image/png;base64,);
	cursor: not-allowed;
}

.igpic.loading, .igpic.next_loading {
	background-image: url(data:image/gif;base64,);
	background-position: center center;
	background-repeat: no-repeat;
	background-color: white;
	cursor: progress;
}

.igpp.delete {
	margin: 0 0 0 10px;
}

.igpp.preview {
	padding: 10px 10px 0 10px;
	position: relative;
}

.igpp.preview .video {
	top: 13px;
	left: 13px;
}

.igpp.preview .igpic_image {
	width: 100px;
	height: 100px;
	border-radius: 3px;
	-webkit-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	-moz-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	overflow: hidden;
	border: #ccc 1px solid;
	margin: 0 10px 10px 0;
	float: left;
}

.igpp.preview .titlefield {
	color: #999;
	font-size: 0.85em;
}

.igpp.preview .caption {
	font-weight: bold;
}

.igpp.preview .igpic_author {
	color: #999999;
	font-size: 0.85em;
}

.igpp.preview .igpic_info {
	margin-bottom: 10px;
	float: left;
}

.igpp.preview .igpic_info p {
	margin: 2px 0;
}

.igpp.halfwidth {
	width: 95%;
	margin-bottom: 10px;
}

.igpp.lookup {
	display: inline;
	margin: 2px 4px 0 -26px;
	background-color: white;
	border-radius: 3px;
	cursor: pointer;
	padding: 3px 5px 2px;
	border: 1px #ccc solid;
	top: 1px;
	position: relative;
}
.igpp.lookup:hover {
	background-color: #ddd;
}

.igpp.lookup img {
	width: 10px;
	height: 10px;
}

.igpp.preview {
	border-radius: 3px;
	-webkit-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	-moz-box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 0 1px 1px rgba(0, 0, 0, 0.1);
	overflow: hidden;
	/*border: #ccc 1px solid;*/
	margin-bottom: 10px;
	display: none;
	/*width: 95%;*/
}

@media only screen
and (min-width: 669px) {
	.igpp.delete {
		margin: 0 10px;
	}
	.igpp.halfwidth {
		width: 65%;
	}
	.igpp.preview {
		/*width: 65%;*/
	}
}

</style>
<div class='igpp preview'></div>
<input class='igpp text halfwidth' type="text" name="{{ name }}" value='{{ value }}'><div class="igpp lookup hidden"><img src="{{ resourceUrl('picpuller/magnifying_glass_icon.svg') }}" alt=""></div><a class="igpp delete icon" title='Remove'></a>
<div class="btn add icon dashed" data-ppbrowsertype='{{ppBrowserType}}'>{{ browseButtonLabel }}{#  (ppBrowserType: {{ppBrowserType}}) #}</div>